    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "python": "cd $dir && python $fileName",
    "php": "cd $dir && php $fileName",
    "perl": "cd $dir && perl $fileName",
    "ruby": "cd $dir && ruby $fileName",
    "go": "cd $dir && go run $fileName",
    "lua": "cd $dir && lua $fileName",
    "groovy": "cd $dir && groovy $fileName",
    "powershell": "cd $dir && powershell -File $fileName",
    "bat": "cd $dir && $fileName",
    "shell": "cd $dir && bash $fileName",
    "fsharp": "cd $dir && fsharpc $fileName && mono $fileNameWithoutExt.exe",
    "csharp": "cd $dir && csc $fileName && $dir$fileNameWithoutExt",
    "haskell": "cd $dir && runhaskell $fileName",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "r": "cd $dir && Rscript $fileName",
    "dart": "cd $dir && dart $fileName",
    "julia": "cd $dir && julia $fileName",
    "kotlin": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
    "scala": "cd $dir && scalac $fileName && scala $fileNameWithoutExt",
    "swift": "cd $dir && swift $fileName",
    "vb": "cd $dir && vbc $fileName && $dir$fileNameWithoutExt",
    "typescript": "cd $dir && tsc $fileName && node $fileNameWithoutExt",
    "javascript": "cd $dir && node $fileName",
    "coffeescript": "cd $dir && coffee $fileName",
    "json": "cd $dir && jq $fileName"
  },
